{
  "name": "infovis",
  "version": "0.3.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "build-bundle": "vue-cli-service build --target lib --name infovis ./src/index.js"
  },
  "dependencies": {
    "ant-design-vue": "^1.3.10",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "d3": "^3.5.3",
    "leaflet": "^1.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-good-wizard": "^1.4.1",
    "vue-lodash": "^2.0.2",
    "vue-router": "^3.1.3",
    "vue-scrollto": "^2.15.0",
    "vue2-leaflet": "^2.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "README.md"
  ],
  "main": "./dist/infovis.common.js",
  "author": "Josefina Estevez",
  "repository": {
    "type": "git",
    "url": "https://github.com/cientopolis/infovis-table"
  },
  "keywords": [
    "information visualization",
    "infovis",
    "table",
    "piechart",
    "barchart",
    "map",
    "multiline",
    "charts"
  ]
}
